<div class="nodeLabelValue">
    <div class="nodeLabel">key</div>
    <input class="nodeValue" type="text"  data-property="key" value="{{key}}" />
</div>
<div class="nodeLabelValue">
    <div class="nodeLabel">model</div>
    <select class="nodeValue" data-property="model">
        {{#select model}}
        {{#each _models}}
        <option value="{{this}}">{{this}}</option>
        {{/each}}
        {{/select}}
    </select>
</div>
<div class="nodeLabelValue">
    <div class="nodeLabel">heat</div>
    <input class="nodeValue" type="text" data-property="temperature" value="{{temperature}}" />
</div>
<div class="nodeLabelValue">
    <div class="nodeLabel">tokens</div>
    <input class="nodeValue" type="text" data-property="maxTokens" value="{{maxTokens}}" />
</div>